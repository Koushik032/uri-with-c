#include<stdio.h>
int main()
{
    double a,b,c,pi=3.14159,area_triangle,area_circle,area_trapijium,area_square,area_rectangle;

    scanf("%lf %lf %lf",&a,&b,&c);

    area_triangle=(0.5*a*c);
    area_circle=pi*c*c;
    area_trapijium=0.5*(a+b)*c;
    area_square=b*b;
    area_rectangle=a*b;

    printf("TRIANGULO: %.3lf\n",area_triangle);
    printf("CIRCULO: %.3lf\n",area_circle);
    printf("TRAPEZIO: %.3lf\n",area_trapijium);
    printf("QUADRADO: %.3lf\n",area_square);
    printf("RETANGULO: %.3lf\n",area_rectangle);

    return 0;
}
